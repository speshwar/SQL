CREATE TABLE STUDENT(
Id INT PRIMARY KEY,
Stud_Name VARCHAR(30),
Gpa DECIMAL(3,2)
);

-- INSERTING THE VALUES
/* NOTE!!!
For the string type Double quotation is necessary
*/
-- INSERTING MULTIPLE VALUES
INSERT INTO STUDENT VALUES
(1,"Aarthi",9.2),
(2,"Anitha",8.0),
(3,"Anitha",8.3),
(4,"Arul",7.5);

-- INSERTING SINGLE VALUES
INSERT INTO STUDENT VALUES (5,"Balaji",9.4);
INSERT INTO STUDENT VALUES (6,"Keerthika",7.6);

-- DISPLAYING THE TABLES
SELECT * FROM STUDENT;

-- INSERTING SPECIFIC VALUES
INSERT INTO STUDENT (Id,Stud_Name) VALUES (7,"Venugopalan");

-- DISPLAYING SPECIFIC VALUES
SELECT Id,Stud_Name FROM STUDENT;
